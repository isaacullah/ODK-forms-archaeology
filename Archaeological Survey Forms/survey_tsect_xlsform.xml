<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>KAAE Survey Transect Record</h:title>
    <model>
      <instance>
        <survey_tsect_xlsform id="KAAE_survey_transect_record">
          <basic_inf>
            <region/>
            <survey_block/>
            <transect_num/>
            <datetime/>
            <surveyor/>
          </basic_inf>
          <other_inf>
            <other_region/>
            <other_surveyor/>
          </other_inf>
          <transect_info>
            <tsect_spacing>20</tsect_spacing>
            <sweep_width>5</sweep_width>
            <visibility/>
          </transect_info>
          <geo_cover>
            <disturbance/>
            <groundcover/>
            <slope/>
            <setting/>
            <sediments/>
          </geo_cover>
          <geo_other>
            <other_disturb/>
            <other_groundcov/>
            <other_setting/>
            <other_sediments/>
          </geo_other>
          <transect_start>
            <start_tsect/>
            <start_image/>
          </transect_start>
          <start_tracklog/>
          <finds>
            <finds_count>
              <pottery_count/>
              <lithics_count/>
              <faunal_count/>
              <brick_count/>
              <tile_count/>
              <other_count/>
            </finds_count>
            <finds_diagnostic>
              <pottery_temporal/>
              <lithics_temporal/>
              <faunal_rem/>
            </finds_diagnostic>
            <pottery>
              <other_pottery/>
              <pot_neol/>
              <pot_chal/>
              <pot_bronze/>
              <pot_iron/>
              <pot_mongol/>
              <pot_medieval/>
              <pot_historic/>
              <pot_modern/>
            </pottery>
            <lithics>
              <other_lithics/>
              <lith_lp/>
              <lith_mp/>
              <lith_up/>
              <lith_epipal/>
              <lith_neol/>
              <lith_bronze/>
              <lith_iron/>
              <lith_mongol/>
              <lith_medieval/>
            </lithics>
            <faunal>
              <other_faunal_rem/>
            </faunal>
            <artif_notes>
              <pottery_notes/>
              <lithic_notes/>
              <faunal_notes/>
              <brick_notes/>
              <tile_notes/>
              <other_notes/>
              <general_notes/>
            </artif_notes>
          </finds>
          <features/>
          <other_features/>
          <arch_features>
            <num_caravans/>
            <num_fire_pits/>
            <num_kurgans/>
            <num_graves/>
            <num_lithic_scats/>
            <num_mounds/>
            <num_other_pits/>
            <num_pithouses/>
            <num_rock_aligns/>
            <num_rock_piles/>
            <num_rock_rings/>
            <num_sherd_scats/>
            <num_standing_stones/>
            <num_stone_walls/>
            <num_towers/>
            <num_tumuli/>
            <num_other_feats/>
          </arch_features>
          <tracklog_end>
            <end_tracklog/>
            <track_name/>
            <tsect_length/>
          </tracklog_end>
          <end_transect>
            <tsect_area/>
            <calculated_area/>
            <end_tsect/>
            <end_image/>
          </end_transect>
          <general_notes/>
          <sketch/>
          <other_images>
            <images jr:template="">
              <general_image/>
            </images>
          </other_images>
          <meta>
            <instanceID/>
          </meta>
        </survey_tsect_xlsform>
      </instance>
      <bind nodeset="/survey_tsect_xlsform/basic_inf/region" required="true()" type="select1"/>
      <bind nodeset="/survey_tsect_xlsform/basic_inf/survey_block" required="true()" type="int"/>
      <bind constraint=". &gt; 0" jr:constraintMsg="Must be a value greater than 0." nodeset="/survey_tsect_xlsform/basic_inf/transect_num" required="true()" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/basic_inf/datetime" required="true()" type="dateTime"/>
      <bind nodeset="/survey_tsect_xlsform/basic_inf/surveyor" required="true()" type="select1"/>
      <bind nodeset="/survey_tsect_xlsform/other_inf/other_region" relevant="selected( /survey_tsect_xlsform/basic_inf/region , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/other_inf/other_surveyor" relevant="selected( /survey_tsect_xlsform/basic_inf/surveyor , &quot;other&quot;)" type="string"/>
      <bind constraint=". &gt; 0" jr:constraintMsg="Must be a value greater than 0." nodeset="/survey_tsect_xlsform/transect_info/tsect_spacing" required="true()" type="decimal"/>
      <bind constraint=". &gt; 0" jr:constraintMsg="Must be a value greater than 0." nodeset="/survey_tsect_xlsform/transect_info/sweep_width" required="true()" type="decimal"/>
      <bind nodeset="/survey_tsect_xlsform/transect_info/visibility" type="select1"/>
      <bind nodeset="/survey_tsect_xlsform/geo_cover/disturbance" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/geo_cover/groundcover" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/geo_cover/slope" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/geo_cover/setting" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/geo_cover/sediments" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/geo_other/other_disturb" relevant="selected( /survey_tsect_xlsform/geo_cover/disturbance , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/geo_other/other_groundcov" relevant="selected( /survey_tsect_xlsform/geo_cover/groundcover , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/geo_other/other_setting" relevant="selected( /survey_tsect_xlsform/geo_cover/setting , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/geo_other/other_sediments" relevant="selected( /survey_tsect_xlsform/geo_cover/sediments , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/transect_start/start_tsect" type="geopoint"/>
      <bind nodeset="/survey_tsect_xlsform/transect_start/start_image" type="binary"/>
      <bind nodeset="/survey_tsect_xlsform/start_tracklog" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_count/pottery_count" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_count/lithics_count" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_count/faunal_count" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_count/brick_count" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_count/tile_count" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_count/other_count" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal" relevant=" /survey_tsect_xlsform/finds/finds_count/pottery_count  &gt;= 1" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal" relevant=" /survey_tsect_xlsform/finds/finds_count/lithics_count  &gt;= 1" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/finds/finds_diagnostic/faunal_rem" relevant=" /survey_tsect_xlsform/finds/finds_count/faunal_count  &gt;= 1" type="select"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/other_pottery" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'other')" type="string"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_neol" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'neol')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_chal" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'chal')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_bronze" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'bronze')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_iron" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'iron')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_mongol" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'mongol')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_medieval" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'medieval')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_historic" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'historic')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/pottery_count " jr:constraintMsg="Your count cannot exceed the total number of pottery counted." nodeset="/survey_tsect_xlsform/finds/pottery/pot_modern" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/pottery_temporal , 'modern')" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/lithics/other_lithics" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'other')" type="string"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_lp" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'lower_paleo')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_mp" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'middle_paleo')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_up" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'upper_paleo')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_epipal" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'epipal')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_neol" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'neol')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_bronze" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'chal')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_iron" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'bronze')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_mongol" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'iron')" type="int"/>
      <bind constraint=". &lt;=  /survey_tsect_xlsform/finds/finds_count/lithics_count " jr:constraintMsg="Your count cannot exceed the total number of lithics counted." nodeset="/survey_tsect_xlsform/finds/lithics/lith_medieval" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/lithics_temporal , 'medieval')" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/finds/faunal/other_faunal_rem" relevant="selected( /survey_tsect_xlsform/finds/finds_diagnostic/faunal_rem , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/pottery_notes" relevant=" /survey_tsect_xlsform/finds/finds_count/pottery_count  &gt;= 1" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/lithic_notes" relevant=" /survey_tsect_xlsform/finds/finds_count/lithics_count  &gt;= 1" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/faunal_notes" relevant=" /survey_tsect_xlsform/finds/finds_count/faunal_count  &gt;= 1" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/brick_notes" relevant=" /survey_tsect_xlsform/finds/finds_count/brick_count  &gt;= 1" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/tile_notes" relevant=" /survey_tsect_xlsform/finds/finds_count/tile_count  &gt;= 1" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/other_notes" relevant=" /survey_tsect_xlsform/finds/finds_count/other_count  &gt;= 1" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/finds/artif_notes/general_notes" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/features" type="select"/>
      <bind nodeset="/survey_tsect_xlsform/other_features" relevant="selected( /survey_tsect_xlsform/features , &quot;other&quot;)" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_caravans" relevant="selected( /survey_tsect_xlsform/features , &quot;caravan&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_fire_pits" relevant="selected( /survey_tsect_xlsform/features , &quot;fire_pit&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_kurgans" relevant="selected( /survey_tsect_xlsform/features , &quot;kurgan&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_graves" relevant="selected( /survey_tsect_xlsform/features , &quot;grave&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_lithic_scats" relevant="selected( /survey_tsect_xlsform/features , &quot;lithic_scat&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_mounds" relevant="selected( /survey_tsect_xlsform/features , &quot;mound&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_other_pits" relevant="selected( /survey_tsect_xlsform/features , &quot;other_pit&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_pithouses" relevant="selected( /survey_tsect_xlsform/features , &quot;pithouse&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_rock_aligns" relevant="selected( /survey_tsect_xlsform/features , &quot;rock_alignment&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_rock_piles" relevant="selected( /survey_tsect_xlsform/features , &quot;rock_pile&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_rock_rings" relevant="selected( /survey_tsect_xlsform/features , &quot;rock_ring&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_sherd_scats" relevant="selected( /survey_tsect_xlsform/features , &quot;sherd_scat&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_standing_stones" relevant="selected( /survey_tsect_xlsform/features , &quot;standing_stone&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_stone_walls" relevant="selected( /survey_tsect_xlsform/features , &quot;stone_wall&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_towers" relevant="selected( /survey_tsect_xlsform/features , &quot;tower&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_tumuli" relevant="selected( /survey_tsect_xlsform/features , &quot;tumuli&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/arch_features/num_other_feats" relevant="selected( /survey_tsect_xlsform/features , &quot;other&quot;)" type="int"/>
      <bind nodeset="/survey_tsect_xlsform/tracklog_end/end_tracklog" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/tracklog_end/track_name" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/tracklog_end/tsect_length" type="decimal"/>
      <bind calculate=" /survey_tsect_xlsform/tracklog_end/tsect_length  *  /survey_tsect_xlsform/transect_info/sweep_width " nodeset="/survey_tsect_xlsform/end_transect/tsect_area" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/end_transect/calculated_area" readonly="true()" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/end_transect/end_tsect" type="geopoint"/>
      <bind nodeset="/survey_tsect_xlsform/end_transect/end_image" type="binary"/>
      <bind nodeset="/survey_tsect_xlsform/general_notes" type="string"/>
      <bind nodeset="/survey_tsect_xlsform/sketch" type="binary"/>
      <bind nodeset="/survey_tsect_xlsform/other_images/images/general_image" type="binary"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/survey_tsect_xlsform/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group appearance="field-list" ref="/survey_tsect_xlsform/basic_inf">
      <label>Basic information.</label>
      <select1 appearance="minimal label" ref="/survey_tsect_xlsform/basic_inf/region">
        <label>Select the region.</label>
        <item>
          <label>Talgar</label>
          <value>talgar</value>
        </item>
        <item>
          <label>Issyk</label>
          <value>issyk</value>
        </item>
        <item>
          <label>Turgen</label>
          <value>turgen</value>
        </item>
        <item>
          <label>Karaturyk</label>
          <value>karaturyk</value>
        </item>
        <item>
          <label>Chilik</label>
          <value>chilik</value>
        </item>
        <item>
          <label>Ash Bulak</label>
          <value>ash_bulak</value>
        </item>
        <item>
          <label>Yunki</label>
          <value>yunki</value>
        </item>
        <item>
          <label>Other...</label>
          <value>other</value>
        </item>
      </select1>
      <input ref="/survey_tsect_xlsform/basic_inf/survey_block">
        <label>Enter the survey block number.</label>
      </input>
      <input ref="/survey_tsect_xlsform/basic_inf/transect_num">
        <label>Enter the transect number</label>
      </input>
      <input ref="/survey_tsect_xlsform/basic_inf/datetime">
        <label>Enter date and a time of transect.</label>
      </input>
      <select1 appearance="minimal label" ref="/survey_tsect_xlsform/basic_inf/surveyor">
        <label>Surveyor:</label>
        <item>
          <label>Claudia</label>
          <value>claudia_chang</value>
        </item>
        <item>
          <label>Perry</label>
          <value>perry_tourtellotte</value>
        </item>
        <item>
          <label>Isaac</label>
          <value>isaac_ullah</value>
        </item>
        <item>
          <label>Sal</label>
          <value>salvador_navarro</value>
        </item>
        <item>
          <label>Other...</label>
          <value>other</value>
        </item>
      </select1>
    </group>
    <group appearance="field-list" ref="/survey_tsect_xlsform/other_inf">
      <label>Enter specifics of “other” in the following categories:</label>
      <input ref="/survey_tsect_xlsform/other_inf/other_region">
        <label>Other region:</label>
      </input>
      <input ref="/survey_tsect_xlsform/other_inf/other_surveyor">
        <label>Other surveyor;</label>
      </input>
    </group>
    <group appearance="field-list" ref="/survey_tsect_xlsform/transect_info">
      <label>Basic transect information</label>
      <input ref="/survey_tsect_xlsform/transect_info/tsect_spacing">
        <label>Enter the spacing of transects (meters)</label>
      </input>
      <input ref="/survey_tsect_xlsform/transect_info/sweep_width">
        <label>Enter the sweep width of the transect (meters)</label>
      </input>
      <select1 appearance="minimal label" ref="/survey_tsect_xlsform/transect_info/visibility">
        <label>Select the most appropriate category for ground visibility.</label>
        <item>
          <label>Good ground visibility (76-100% visible)</label>
          <value>good</value>
        </item>
        <item>
          <label>Moderate ground visibility (51-75% visible)</label>
          <value>moderate</value>
        </item>
        <item>
          <label>Poor ground visibility (26-50% visible)</label>
          <value>poor</value>
        </item>
        <item>
          <label>Very poor ground visibility (1-25% visible)</label>
          <value>very_poor</value>
        </item>
        <item>
          <label>Cannot see the ground (0% visibility)</label>
          <value>none</value>
        </item>
